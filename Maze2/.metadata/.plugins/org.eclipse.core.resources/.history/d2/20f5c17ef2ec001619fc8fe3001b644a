package Snippet;
/*
 * This problem is from geeksforgeeks. 
 * The question in the leetcode was closed. 
 */

public class Maze2 {
	
	public int[][] path(int[][] mat){
		int [][] path = new int[mat.length][mat[0].length];
		
	}
	
	private void DFS(int row, int col, int[][] path, int mat[][]){
		if(row<0 || row>=mat.length){ return; }
		if(col<0 || col>=mat[0].length){ return; }
		if(row==(mat.length-1) && col==(mat[0].length-1)){
			path[row][col] = 1;
		}
		
	}
}
